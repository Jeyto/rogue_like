CC=gcc
FLAG=-Wall -W
OBJ= main.o liste.o
JOB= ./include/*.h
SRC=./src/
all:main.exe clean

main.exe: ${OBJ}
	${CC} -o ./bin/main.exe ${OBJ} ${FLAG}

struct.o: ${SRC}struct.c ${JOB}
	${CC} -c ${SRC}struct.c ${FLAG}

liste.o: ${SRC}liste.c ${JOB}
	${CC} -c ${SRC}liste.c ${FLAG} 

main.o: ${SRC}main.c ${JOB}
	${CC} -c ${SRC}main.c ${JOB} ${FLAG}
	
clean:
	erase  *.o

